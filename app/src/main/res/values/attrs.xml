<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FormInputView">
        <attr name="input_level" format="integer" />
        <attr name="regex" format="string" />
        <attr name="error_text" format="string" />
        <attr name="android:inputType" />
        <attr name="android:imeOptions" />
        <attr name="android:hint" />
        <attr name="android:imeActionId" />
        <attr name="android:imeActionLabel" />
    </declare-styleable>

    <declare-styleable name="SgTabView">
        <attr name="tab_text" format="reference" />
        <attr name="tab_icon" format="reference" />
        <attr name="tab_checked_color" format="reference" />
        <attr name="tab_unchecked_color" format="reference" />
        <attr name="tab_checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SgEditText">
        <attr name="bordered" format="boolean" />
    </declare-styleable>

</resources>